#!/bin/sh
#
# This script will attempt to store a container image on docker hub
# to be used when launching cloud.gov images
#

[ -f ./local-envs-rc ] && . ./local-envs-rc
[ -f ./container-type-rc ] && . ./container-type-rc

echo "docker push $DOCKERUSER/$DOCKERREPO:${CONTAINERTYPE}-$CONTAINERTAG"
docker push $DOCKERUSER/$DOCKERREPO:${CONTAINERTYPE}-$CONTAINERTAG
if [ -n "$CONTAINERTAG2" ]
then
    echo "docker push $DOCKERUSER/$DOCKERREPO:${CONTAINERTYPE}-$CONTAINERTAG2"
    docker push $DOCKERUSER/$DOCKERREPO:${CONTAINERTYPE}-$CONTAINERTAG2
fi
